{
  "manifest_version": 3,
  "name": "Melvor 1.2 AutoLooter",
  "version": "1.1",
  "description": "Melvor 1.2 AutoLooter is a must-have for avid gamers who hate missing out on precious loot. AutoLooter comes to the rescue!",
  "content_scripts": [
    {
      "matches": [
        "https://melvoridle.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
  ],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "24": "icon.png",
      "32": "icon.png"
    },
    "default_title": "Melvor 1.2 AutoLooter",
    "default_popup": "popup.html"
  }
}