{
  "manifest_version": 3,
  "name": "Melvor Idle 1.2 Auto eater",
  "version": "1.1",
  "description": "Melvor 1.2 Autoeater helps your character to survive by eating food when your health is low.",
  "content_scripts": [
    {
      "matches": [
        "https://melvoridle.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
  ],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "24": "icon.png",
      "32": "icon.png"
    },
    "default_title": "Melvor 1.2 Autoeater",
    "default_popup": "popup.html"
  }
}